<web-app>
    <display-name>
        CNT 4714 Spring 2023 3Tier Architecture
    </display-name>
    <description>
        3 Tier DBMS Web Application
    </description>

    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>AuthenticationServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>clientuserservlet</servlet-name>
        <servlet-class>ClientUserServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>clientuserservlet</servlet-name>
        <url-pattern>/ClientUserApp</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>rootuserservlet</servlet-name>
        <servlet-class>RootUserServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>rootuserservlet</servlet-name>
        <url-pattern>/RootUserApp</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>suppliersinsertservlet</servlet-name>
        <servlet-class>SuppliersInsertServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>suppliersinsertservlet</servlet-name>
        <url-pattern>/Suppliers</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>partsinsertservlet</servlet-name>
        <servlet-class>PartsInsertServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>partsinsertservlet</servlet-name>
        <url-pattern>/Parts</url-pattern>
    </servlet-mapping>
</web-app>