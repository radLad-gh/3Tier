<web-app>
    <display-name>
        CNT 4714 Spring 2023 3Tier Architecture
    </display-name>
    <description>
        3 Tier DBMS Web Application
    </description>

    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>AuthenticationServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>clientuserservlet</servlet-name>
        <servlet-class>ClientUserServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>clientuserservlet</servlet-name>
        <url-pattern>/clientuserservlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>rootuserservlet</servlet-name>
        <servlet-class>RootUserServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>rootuserservlet</servlet-name>
        <url-pattern>/rootuserservlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>dataentryservlet</servlet-name>
        <servlet-class>DataEntryServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>dataentryservlet</servlet-name>
        <url-pattern>/dataentryservlet</url-pattern>
    </servlet-mapping>
</web-app>