<web-app>
    <display-name>
        CNT 4714 Spring 2023 3Tier Architecture
    </display-name>
    <description>
        3 Tier DBMS Web Application
    </description>

    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>T1.AuthenticationServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>

    <error-page>
        <error-code>403</error-code>
        <location>/WEB-INF/jsp/403.jsp</location>
    </error-page>
</web-app>